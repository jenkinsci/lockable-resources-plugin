<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:optionalBlock title="${%This build uses dynamic resources}"
                     name="dynamic-resources-prop"
                     checked="${instance!=null}">
        <f:nested>
            <f:entry title="${%Create dynamic resources for other builds}" field="createDynamicResources">
                <f:checkbox/>
            </f:entry>
            <f:entry title="${%Generate dynamic resources for}" field="generatedForJobs">
                <f:textbox/>
            </f:entry>
            <f:entry title="${%Consume dynamic resources from other builds}" field="consumeDynamicResources">
                <f:checkbox/>
            </f:entry>
            <f:entry title="${%Wait for dynamic resources generated by}" field="generatedByJobs">
                <f:textbox/>
            </f:entry>
            <f:entry title="${%Ignored matrix configuration axis}" field="ignoredAxis">
                <f:textbox/>
            </f:entry>
        </f:nested>
    </f:optionalBlock>
</j:jelly>
